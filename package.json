{
  "name": "gitgud",
  "version": "2.0.0",
  "description": "This extension adds hot keys and hierarchical file view to GitHub pull request pages. Also show warnings from a pre-defined checklist",
  "main": "main.js",
  "devDependencies": {
    "async": "^2.0.0-rc.5",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "requirejs": "^2.2.0",
    "rimraf": "^3.0.0",
    "uglify-js": "^2.7.3",
    "zip-folder": "^1.0.0"
  },
  "scripts": {
    "build": "node build.js",
    "watch": "npm-watch"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,html,css,json"
    }
  },
  "author": "Le Manh Dat",
  "repository": "https://github.com/datlm-1340/gitgud",
  "license": "ISC",
  "dependencies": {
    "lodash": "^4.17.20",
    "npm-watch": "^0.7.0",
    "rimraf": "^3.0.2"
  }
}
